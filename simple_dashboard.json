{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus instance containing nextcloud-exporter metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__elements": {},
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.2.3" },
    { "type": "panel", "id": "piechart", "name": "Pie chart", "version": "" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "datasource", "uid": "grafana" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": { "limit": 100, "matchAny": false, "tags": [], "type": "dashboard" },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 2,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 5,
      "panels": [],
      "title": "Users and Files",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 4, "x": 0, "y": 1 },
      "id": 12,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto",
        "wideLayout": true
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "editorMode": "code",
          "expr": "sum by(instance) (nextcloud_active_users_daily_total{instance=\"$instance\"})",
          "legendFormat": "__auto",
          "refId": "A",
          "range": true
        }
      ],
      "title": "Daily Active Users",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 0, "lineInterpolation": "linear", "lineWidth": 1, "showPoints": "never", "stacking": { "group": "A", "mode": "none" } },
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 8, "x": 4, "y": 1 },
      "id": 1,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": false },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(instance) (nextcloud_users_total{instance=\"$instance\"})",
          "legendFormat": "total ({{instance}})",
          "refId": "A",
          "range": true
        },
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(instance) (nextcloud_active_users_total{instance=\"$instance\"})",
          "legendFormat": "active ({{instance}})",
          "refId": "B",
          "range": true
        }
      ],
      "title": "Users",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": { "drawStyle": "line", "fillOpacity": 0, "lineInterpolation": "linear", "lineWidth": 1, "showPoints": "never", "stacking": { "group": "A", "mode": "none" } },
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 1 },
      "id": 11,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": false },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by(instance) (nextcloud_files_total{instance=\"$instance\"})",
          "legendFormat": "__auto",
          "refId": "B",
          "range": true
        }
      ],
      "title": "Files",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 8 },
      "id": 6,
      "panels": [],
      "title": "Shares",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 9 },
      "id": 2,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(type) (nextcloud_shares_total{instance=\"$instance\"})", "legendFormat": "{{type}}", "refId": "A", "range": true }
      ],
      "title": "Shares by Type",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 9 },
      "id": 3,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(direction) (nextcloud_shares_federated_total{instance=\"$instance\"})", "legendFormat": "{{direction}}", "refId": "A", "range": true }
      ],
      "title": "Federated Shares",
      "type": "timeseries"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 16 },
      "id": 7,
      "panels": [],
      "title": "Apps",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 0, "y": 17 },
      "id": 9,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "auto",
        "wideLayout": true
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(instance) (nextcloud_apps_installed_total{instance=\"$instance\"})", "refId": "A", "range": true }
      ],
      "title": "Installed Apps",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "thresholds" }, "unit": "short" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 4, "y": 17 },
      "id": 10,
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "textMode": "auto",
        "wideLayout": true
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(instance) (nextcloud_apps_updates_available_total{instance=\"$instance\"})", "refId": "A", "range": true }
      ],
      "title": "Available Updates",
      "type": "stat"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "id": 8,
      "panels": [],
      "title": "System Information",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "decbytes" }, "overrides": [] },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 25 },
      "id": 4,
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom", "showLegend": false },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(instance) (nextcloud_free_space_bytes{instance=\"$instance\"})", "refId": "A", "range": true }
      ],
      "title": "Disk Space Left",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 12, "y": 25 },
      "id": 13,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["percent"] },
        "pieType": "pie",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "single", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(version) (nextcloud_system_info)", "instant": true, "refId": "A", "range": false }
      ],
      "title": "Nextcloud Versions (all instances)",
      "type": "piechart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 16, "y": 25 },
      "id": 17,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["percent"] },
        "pieType": "pie",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "single", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(version) (nextcloud_exporter_info)", "instant": true, "refId": "A", "range": false }
      ],
      "title": "Exporter Versions (all instances)",
      "type": "piechart"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 32 },
      "id": 16,
      "panels": [],
      "title": "Environment Information",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 0, "y": 33 },
      "id": 14,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["percent"] },
        "pieType": "pie",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "single", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(version) (nextcloud_php_info)", "instant": true, "refId": "A", "range": false }
      ],
      "title": "PHP Versions (all instances)",
      "type": "piechart"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "gridPos": { "h": 7, "w": 4, "x": 4, "y": 33 },
      "id": 15,
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "showLegend": true, "values": ["percent"] },
        "pieType": "pie",
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "tooltip": { "mode": "single", "sort": "desc" }
      },
      "targets": [
        { "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" }, "expr": "sum by(type, version) (nextcloud_database_info)", "instant": true, "legendFormat": "{{type}} {{version}}", "refId": "A", "range": false }
      ],
      "title": "Databases (all instances)",
      "type": "piechart"
    }
  ],
  "refresh": "5m",
  "schemaVersion": 39,
  "tags": ["prometheus","nextcloud"],
  "templating": {
    "list": [
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(nextcloud_up, instance)",
        "refresh": 2,
        "includeAll": false,
        "multi": false
      }
    ]
  },
  "time": { "from": "now-30d", "to": "now" },
  "timepicker": {
    "refresh_intervals": ["30s","1m","5m","15m","1h","2h","1d"],
    "time_options": ["5m","15m","1h","6h","12h","24h","2d","7d","30d"]
  },
  "timezone": "browser",
  "title": "Nextcloud",
  "description": "Dashboard showing metrics gathered by nextcloud-exporter from a Nextcloud instance."
}

